{
  "presets": [
    "es2015-native-modules",
    "stage-2",
    "react"
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"],
      "plugins": [["react-transform", {
        "transforms": [{
          "transform": "react-transform-catch-errors",
          "imports": [
            "react",
            "redbox-react"
          ]
        }]
      }]]
    }
  },
  "plugins": ["transform-class-properties"]
}
